{
  "$schema": "https://schemas.tauri.app/config/2/capabilities",
  "identifier": "default",
  "description": "Default capabilities for iToDo",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "notification:default",
    "dialog:default",
